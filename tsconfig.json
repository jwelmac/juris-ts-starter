{
  "compilerOptions": {
    "target": "ES2020",
    "module": "nodenext",
    "strict": true,
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
    "include": ["**/*.js", "**/*.ts", "**/*.d.ts", "types/**/*"],
    "exclude": ["node_modules"]
}